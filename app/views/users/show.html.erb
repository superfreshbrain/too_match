<div class="container">
  <div class="row">
  <p><%= @username %>さんの投稿一覧</p>
  <% @posts.each do |post| %>
    <div class="content_post" style="background-image: url(<%= post.image %>);">
      <%= simple_format(post.description) %>
      <span class="name">
        <a href="">
          <span>投稿者</span><%= post.user.username %>
        </a>
      </span>
    </div>
  <% end %>
  <%= paginate(@posts) %>
  </div>
</div>
